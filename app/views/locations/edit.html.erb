<%= form_for @location do |f| %>
    <%= render partial: "layouts/errors", locals: {model: @location} %>
  
      <div id="c_location">
      <%= f.label :street %>
      <%= f.text_field :street %><br>
      <%= f.label :city %>
      <%= f.text_field :city %><br>
      <%= f.label :state %>
      <%= f.text_field :state %><br>
      <%= f.label :zip_code %>
      <%= f.text_field :zip_code %><br><br>
      <%= f.submit %>
      </div>
  <% end %>