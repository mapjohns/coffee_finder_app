<h1>Welcome Back! Please Sign-in below to view your coffee profile!</h1>
<%= form_tag '/signin' do %>
    <label>Username: </label><br>
    <%= text_field_tag :'user[:username]' %><br>

    <label>Password: </label><br>
    <%= text_field_tag :'user[:password]' %><br><br>

    <%= submit_tag "Log in" %>
<% end %>
